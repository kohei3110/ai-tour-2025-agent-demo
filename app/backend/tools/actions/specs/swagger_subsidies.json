        "additionalProperties": true,
        "required": ["metadata", "result"],
        "properties": {
          "metadata": {
            "type": "object",
            "additionalProperties": true,
            "required": ["type", "resultset"],
            "properties": {
              "type": {
                "description": "API仕様公開URL（API Specification URL）",
                "example": "https://anypoint.mulesoft.com/exchange/497c75e7-5d6a-48b3-a39b-4105316f61f0/exp-public/",
                "type": "string"
              },
              "resultset": {
                "type": "object",
                "additionalProperties": true,
                "required": ["count"],
                "properties": {
                  "count": {
                    "description": "取得件数（Number of results）",
                    "example": 1,
                    "type": "integer",
                    "format": "int"
                  }
                }
              }
            }
          },
          "result": {
            "description": "補助金一覧検索結果情報（Result of querying subsidies list）",
